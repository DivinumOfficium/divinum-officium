[Param]
Priest~>$priest~>Checkbutton;;
Church patron~>$churchpatron~>Entry~>12;;
Building script~>$building~>Checkbutton;;
Language1~>$lang1~>Optionmenu~>languages;;
Psalmversion~>$accented~>Optionmenu~>psalmversions;;
Border width~>$border~>Entry~>3;;
Scroll amount~>$scrollamount~>Entry~>3;;

[Colors]
Black & cell~>$blackcell~>Font;;
Small red~>$redsmall~>Font1;;
Title & frame~>$titlelargeframe~>Font;;
Link font~>$linkfont~>Font3;;
Dialog font~>$dialogfont~>Font5;;
Grey 1~>$voicegrey1~>Color;;
Grey 2~>$voicegrey2~>Color;;

[Chant]
Key frequency~>$dofreq~>Scaleentry~>100~300~5;;
Syllable time~>$basetime~>Scaleentry~>100~700~10;;
Vowel mult / 10~>$voweltime~>Scaleentry~>10~100~2;;
Vowel fill perc~>$vowelperc~>Scaleentry~>30~150~10;;

[Voice]
continuous~>$voicecontinue~>Checkbutton;;
pitch1~>$pitch1~>Scaleentry~>-10~10~1;;
voice1~>$voicename1~>Optionmenu~>@voicenames;;
pitch2~>$pitch2~>Scaleentry~>-10~10~1;;
voice2~>$voicename2~>Optionmenu~>@voicenames;;
voice rate~>$voicerate~>Scaleentry~>-10~10~1;;
volume~>$volume~>Scaleentry~>10~100~10;;

[general]
Expand~>$expand~>Optionmenu~>{all,psalms,none,skeleton};;
Version~>$version~>Optionmenu~>{Tridentinum,Divino Afflatu,Reductions 1955,Rubrics 1960};;
Language 2~>$lang2~>Optionmenu~>languages

